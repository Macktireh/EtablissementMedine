				{
					"name": "Logout",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"console.log()\r",
									"\r",
									"const baseUrl = pm.environment.get(\"baseUrl\")\r",
									"\r",
									"pm.sendRequest({\r",
									"    url: `${baseUrl}/api/v1/auth/user/token/verify/`,\r",
									"    method: \"POST\",\r",
									"    header: {\r",
									"        \"Accept\": \"application/json\",\r",
									"        \"Content-Type\": \"application/json\"\r",
									"    },\r",
									"    body: JSON.stringify({ token: pm.environment.get(\"accessToken\") })\r",
									"}, (error, response) => {\r",
									"    console.log(\"STATUS CODE :\", response.code)\r",
									"    if (response.code === 401) {\r",
									"        console.log(\"VERIFY TOKEN FAIL\")\r",
									"        console.log()\r",
									"        pm.sendRequest({\r",
									"            url: `${baseUrl}/api/v1/auth/user/token/refresh/`,\r",
									"            method: \"POST\",\r",
									"            header: {\r",
									"                \"Accept\": \"application/json\",\r",
									"                \"Content-Type\": \"application/json\"\r",
									"            },\r",
									"            body: JSON.stringify({ refresh: pm.environment.get(\"refreshToken\") })\r",
									"        }, (error, response) => {\r",
									"            if (response.code === 401) {\r",
									"                console.log(\"STATUS CODE :\", response.code)\r",
									"                console.log(\"REFRESH TOKEN FAIL\")\r",
									"            } else if (response.code === 200) {\r",
									"                console.log(\"STATUS CODE :\", response.code)\r",
									"                console.log(\"REFRESH TOKEN SUCCESS\")\r",
									"                pm.environment.set('accessToken', response.json().access)\r",
									"                console.log(\"RES\", response)\r",
									"            } else {\r",
									"                console.log(\"ELSE :\")\r",
									"                console.log(\"STATUS CODE :\", response.code)\r",
									"                console.log(\"RES\", response)\r",
									"            }\r",
									"        })\r",
									"    } else if (response.code === 200) {\r",
									"        console.log(\"VERIFY TOKEN SUCCESS\")\r",
									"        console.log(\"RES\", response)\r",
									"    } else {\r",
									"        console.log(\"ELSE :\")\r",
									"        console.log(\"STATUS CODE :\", response.code)\r",
									"        console.log(\"RES\", response)\r",
									"    }\r",
									"});"
								],
								"type": "text/javascript"
							}
						},
